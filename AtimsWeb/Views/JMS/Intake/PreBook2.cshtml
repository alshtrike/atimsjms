
    <div class="form-group">
        <label class="col-sm-2 control-label">Age</label>
        <div class="col-sm-10">
            <input type="number" class="form-control validate" min="1" ng-model="newInmate.Person.person_age" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Date of Birth</label>
        <div class="col-sm-10">
            <input type="date" class="form-control" ng-model="newInmate.Person.person_dob" />
        </div>
    </div>
    <div class="col-sm-offset-2 col-sm-10">
        <input type="button" value="Previous" class="btn btn-default"  ng-click="updateProgress('previous')" />
        <input type="submit" ng-click="updateProgress('next')" ng-disabled="preBookForm.$invalid" class="btn btn-default" value="Next" />
    </div>

