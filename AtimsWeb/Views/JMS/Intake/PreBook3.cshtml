
    <div class="form-group">
        <label class="col-sm-2 control-label">Inmate Number</label>
        <div class="col-sm-10">
            <input type="number" class="form-control validate" min="1" ng-model="newInmate.inmate_number" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-sm-2 control-label">Date Recieved</label>
        <div class="col-sm-10">
            <input type="date" class="form-control" ng-model="newInmate.received_date" />
        </div>
    </div>

    <div class="col-sm-offset-2 col-sm-10">
        <input type="button" value="Previous" class="btn btn-default" ng-click="updateProgress('previous')" />
        <input type="submit" ng-click="updateProgress('next')" ng-disabled="preBookForm.$invalid" class="btn btn-default" value="Next" />
    </div>


